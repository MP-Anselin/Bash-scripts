 AND s1.niveau ='1'
 AND s3.code_asso_pere = s2.code_asso
 AND s2.code_asso_pere = s1.code_asso
 AND e1.libelle = p_lib_1
 AND e2.libelle = p_lib_2
 AND e3.libelle = p_lib_3;
 EXCEPTION WHEN NO_DATA_FOUND THEN
 NULL;
 END;
 
 IF (p_obj_theme.code_asso = 0) THEN
