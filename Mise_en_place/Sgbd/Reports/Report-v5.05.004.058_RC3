=SQL=
create or replace FUNCTION ADM_ADD_SCH_SUJET( p_code_schema            IN NUMBER, p_lib_1                  IN VARCHAR2,
 p_lib_2                  IN VARCHAR2,
 p_lib_3                  IN VARCHAR2,
 p_lib_4                  IN VARCHAR2,
