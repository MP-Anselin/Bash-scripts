##
STD_DPAE_MSA_HISTORIQUE
=SQL IFNOTEXIST=STD_DPAE_MSA_HISTORIQUE=
CREATE TABLE STD_DPAE_MSA_HISTORIQUE(DPAE_ID NUMBER(4) NOT NULL ENABLE, DATE_MAJ DATE, CODE_STATUT VARCHAR2(1), UTILISATEUR VARCHAR2(32),CONSTRAINT FK_STD_DPAE_MSA_HISTORIQUE FOREIGN KEY (DPAE_ID) REFERENCES DPAE_MSA(DPAE_ID) ON DELETE CASCADE)
