STD_IMP_DOSSIER_DONNEES_TMP
=SQL IFNOTEXIST=STD_IMP_DOSSIER_DONNEES_TMP=
CREATE TABLE STD_IMP_DOSSIER_DONNEES_TMP AS SELECT * FROM STD_IMP_DOSSIER_DONNEES
##
STD_IMP_DOSSIER_DONNEES
=SQL IFEXIST=STD_IMP_DOSSIER_DONNEES=
DROP TABLE STD_IMP_DOSSIER_DONNEES
##
STD_IMP_DOSSIER_DONNEES
=SQL IFNOTEXIST=STD_IMP_DOSSIER_DONNEES=
CREATE TABLE STD_IMP_DOSSIER_DONNEES(ID_TRT NUMBER(6,0),ID_LIGNE NUMBER(10,0),ID_DOSSIER NUMBER(6,0),DATE_AJOUT TIMESTAMP (6),MATRIC VARCHAR2(32 BYTE), MATREP VARCHAR2(11 BYTE), DATE_EFF DATE, CARTE VARCHAR2(3 BYTE),ACTION VARCHAR2(2 BYTE),DONNEES CLOB,PRIMARY KEY (ID_TRT, ID_LIGNE))
##
STD_IMP_DOSSIER_DONNEES
=SQL IFEXIST=STD_IMP_DOSSIER_DONNEES=
INSERT INTO STD_IMP_DOSSIER_DONNEES SELECT * FROM STD_IMP_DOSSIER_DONNEES_TMP
